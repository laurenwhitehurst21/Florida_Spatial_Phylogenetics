# Sankey Diagram
See https://r-graph-gallery.com/321-introduction-to-interactive-sankey-diagram-2.html
```{r}
library(networkD3)
library(dplyr)

gators_cleaning <- read.csv("../gapplied/data/gators_cleaning.csv")
gators_cleaning <- gators_cleaning[,c(2:5)]
gatorsums= c(sum(raw_count[,3]),
             sum(gators_cleaning[,2]),
             sum(gators_cleaning[,3]),
             sum(gators_cleaning[,4])
)

#write.csv(gator_round_up, file = "data/gator_round_up.csv")
links <- data.frame(
  source=c("gators_download()", "taxa_clean()", "taxa_clean()", 
           "basic_locality_clean()", "basic_locality_clean()",
           "remove_duplicates()", "remove_duplicates()"),
  target=c("taxa_clean()", "basic_locality_clean()", 
           "Records Removed", "remove_duplicates()", "Records Removed",
           "Records Retained", "Records Removed"),
  values= c(gatorsums[1],
            gatorsums[2],
            gatorsums[1] - gatorsums[2],
            gatorsums[3],
            gatorsums[2]- gatorsums[3],
            gatorsums[4],
            gatorsums[3] - gatorsums[4]))
# From these flows we need to create a node data frame: it lists every entities involved in the flow
nodes <- data.frame(
  name=c(as.character(links$source), 
         as.character(links$target)) %>% unique()
)

# With networkD3, connection must be provided using id, not using real name like in the links dataframe.. So we need to reformat it.
links$IDsource <- match(links$source, nodes$name)-1 
links$IDtarget <- match(links$target, nodes$name)-1

nodes$group <- as.factor(c("a","a","a","a","a","b"))

setwd("/blue/soltis/laurenwhitehurst/FL_Phylogenetics")


my_color <- 'd3.scaleOrdinal() .domain(["a", "b"]) .range(["#0021A5", "#FA4616"])'
# Make the Network
sankeyNetwork(Links = links, Nodes = nodes,
              Source = "IDsource", Target = "IDtarget",
              Value = "values", NodeID = "name", NodeGroup = "group",
              sinksRight=FALSE,fontSize = 12, nodeWidth = 20, colourScale = my_color, 
              width = 800, height = 300)



```



```{r}

library(networkD3)
library(dplyr)

spocc_cleaning <- read.csv("../gapplied/data/spocc_cleaning.csv")
spocc_cleaning <- spocc_cleaning[,c(2:5)]

# Order 
#ord = match(raw_count$Scientific_Name, spocc_cleaning$V1)
#spocc_cleaning =  spocc_cleaning[ord,]
#colnames(spocc_cleaning) <- c("Scientific Name", "Taxanomic.Cleaning", "Locality.Cleaning", #"Full.Clean" )
#spocc_round_up <- as.data.frame(c(raw_count[,c(1,2)], spocc_cleaning[,c(2:4)]))
#write.csv(spocc_round_up, file = "data/spocc_round_up.csv")
spoccsums= c(sum(raw_count[,2]),
             sum( spocc_cleaning[,2]),
             sum( spocc_cleaning[,3]),
             sum( spocc_cleaning[,4])
)
links2 <- 
  data.frame(
    source=c("occ()", "taxa_clean()", "taxa_clean()", 
             "basic_locality_clean()", "basic_locality_clean()",
             "remove_duplicates()", "remove_duplicates()"),
    target=c("taxa_clean()", "basic_locality_clean()", 
             "Records Removed", "remove_duplicates()", "Records Removed",
             "Records Retained", "Records Removed"),
    values= c(spoccsums[1],
              spoccsums[2],
              spoccsums[1] - spoccsums[2],
              spoccsums[3],
              spoccsums[2]- spoccsums[3],
              spoccsums[4],
              spoccsums[3] - spoccsums[4]))
# From these flows we need to create a node data frame: it lists every entities involved in the flow
nodes2 <- data.frame(
  name=c(as.character(links2$source), 
         as.character(links2$target)) %>% unique()
)

# With networkD3, connection must be provided using id, not using real name like in the links dataframe.. So we need to reformat it.
links2$IDsource <- match(links2$source, nodes2$name)-1 
links2$IDtarget <- match(links2$target, nodes2$name)-1

nodes2$group <- as.factor(c("a","a","a","a", "a","b"))


my_color <- 'd3.scaleOrdinal() .domain(["a", "b"]) .range(["#0021A5", "#FA4616"])'
# Make the Network
sankeyNetwork(Links = links2, Nodes = nodes2,
              Source = "IDsource", Target = "IDtarget",
              Value = "values", NodeID = "name", NodeGroup = "group",
              sinksRight=FALSE,fontSize = 12, nodeWidth = 20, colourScale = my_color,
              width = 800, height = 300)

