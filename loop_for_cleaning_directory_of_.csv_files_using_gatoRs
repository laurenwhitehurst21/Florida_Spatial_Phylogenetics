## Create Synonymn Lists   
Verbena_brasiliensis<-c("Verbena brasiliensis","Verbena chacensis","Verbena hansenii","Verbena paucifolia")
Verbascum_thapsus<-c("Verbascum thapsus","Verbascum alatum","Verbascum angustius","Verbascum canescens","Verbascum collinum","Verbascum cuspidatum","Verbascum decurrens","Verbascum elongatum","Verbascum indicum","Verbascum linnaei","Verbascum neglectum","Verbascum officinarum","Verbascum pallidum","Verbascum plantagineum","Verbascum schraderi","Verbascum seminigrum","Verbascum simplex","Verbascum spectabile","Verbascum visianianum")
Woodwardia_areolata<-c("Woodwardia areolata","Woodwardia angustifolia","Lorinseria areolata","Osmunda caroliniana","Woodwardia floridana")
Rumex_obtusifolius<-c("Rumex obtusifolius","Rumex crispatulus","Acetosa obtusifolia","Lapathum obtusifolium","Lapathum sylvestre","Rumex sylvestris")


# Make list of synonyms 
l.df <- lapply(ls(pattern = "*_*"), function(x) get(x))
name <- c("Verbena_brasiliensis",
          "Verbascum_thapsus","Woodwardia_areolata") 
#name <- c("Asclepias_curtissii","Calamintha_ashei","Dicerandra_frutescens","Encyclia_tampensis","Eriogonum_longifolium_var_gnaphalifolium","Eryngium_cuneifolium","Garberia_heterophylla","Hartwrightia_floridana","Hypericum_cumulicola","Hypericum_edisonianum","Lechea_cernua","Liatris_ohlingerae","Lilium_catesbaei","Nolina_brittoniana","Panicum_abscissum","Paronychia_chartacea","Pinguicula_lutea","Platanthera_ciliaris","Polygonum_basiramia","Polygonum_myriophylla","Prunus_geniculata","Tillandsia_balbisiana","Tillandsia_fasciculata","Tillandsia_utriculata","Warea_carteri")
name <- gsub("_", " ", name)
names(l.df) <- name
#save(l.df, file = "data/synonym.RData")



## Download records    

### Download with spocc   

occout <- c()
for(i in 1:17){
  occout[[i]] <-  spocc::occ2df(spocc::occ(query = l.df[[i]], from = c("gbif", "idigbio")))
}
save(occout, file = "../../Gatorsapplied/gapplied/data/occout.RData")
```

### Download with gatoRs   
gout <- c()
df<-gout[[15]]
for(i in 1:18){
  gout[[i]] <-  gatoRs::gators_download(synonyms.list  = l.df[[i]], 
                                        write.file = FALSE, 
                                        gbif.match = "fuzzy",
                                        idigbio.filter = TRUE)
}

save(gout, file = "../Gatorsapplied/gapplied/data/gout.RData")
```

raw_count <- read.csv("../data/raw_count.csv")

```

# Cleaning
```{r  eval=FALSE, include=TRUE}
df<-(i in gout)
temp <- full_clean(df,
                   synonyms.list = c("Triticum aestivum","Triticum aegyptiacum","Triticum aestivum","Triticum cereale","Triticum compactum","Triticum creticum","Triticum hybernum","Triticum imberbe","Triticum orientale","Triticum persicum","Triticum pyramidale","Triticum sativum","Triticum segetale","Frumentum triticum","Triticum vulgare","Zeia vulgaris"),
                   remove.NA.occ.id = TRUE,
                   remove.NA.date = FALSE,
                   accepted.name =  "Triticum aestivum",
                   remove.zero = TRUE,
                   precision = TRUE,
                   digits = 2,
                   remove.skewed = TRUE,
                   basis.list = c("PRESERVED_SPECIMEN", "PreservedSpecimen", "Preserved Specimen"),
                   remove.flagged = TRUE,
                   thin.points = TRUE,
                   distance = 0.002,
                   reps = 100,
                   one.point.per.pixel = TRUE)
write.csv(temp, "/blue/soltis/laurenwhitehurst/FL_Phylogenetics/Cleaned_occurence_data/Triticum_aestivum.csv") 

gators_cleaning <- data.frame()
for(i in 1:17){
  gators_cleaning[i, 1] <- names(l.df[i])
  # Taxa clean
 goutclean[[i]] <- taxa_clean(df = gout[[i]],  
                               synonyms.list = l.df[[i]], 
                               taxa.filter = "fuzzy", 
                               accepted.name =  names(l.df[i])) 

  # Basic Locality Clean 

  goutclean[[i]] <- basic_locality_clean(df = goutclean[[i]],
                                         latitude = "latitude",
                                         longitude = "longitude",
                                         remove.zero = TRUE,
                                         precision = TRUE,
                                         digits = 3,
                                         remove.skewed = FALSE)
  
  gators_cleaning[i, 3] <- nrow(goutclean[[i]])
  
  # Remove dups
  occoutclean[[i]] <- remove_duplicates(df = occoutclean[[i]],
                                        event.date = "date",
                                        latitude = "latitude",
                                        longitude = "longitude", 
                                        aggregator = "prov", 
                                        id = "key")
  goutclean[[i]] <- remove_duplicates(df = goutclean[[i]])
    gators_cleaning[i, 4] <- nrow(goutclean[[i]])
  
}

for(i in 1:25){
  gators_cleaning[i, 4] <- nrow(goutclean[[i]])
}
#write.csv(gators_cleaning, "../gapplied/data/gators_cleaning.csv")
#write.csv(spocc_cleaning, "../gapplied/data/spocc_cleaning.csv")

